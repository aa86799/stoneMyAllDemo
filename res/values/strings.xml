<!-- 
	& 对应 &amp;
	< 对应 &lt;
	> 对应 &gt;
	' 对应 &apos;
	" 对应 &quot; 
-->
	<!-- getResources().getConfiguration().locale 得出当前本地化对象 -->
	 <!-- 1$第1 个占位符 2$第二个 getString(R.string.format_string, "abc", 98.33);-->
	<string name="format_string">a string %1$s, a decimal %2$f</string>
	
	<plurals name="planes">
	    <item quantity="zero">none</item>
	    <item quantity="one">plane %d</item>
	    <item quantity="other">%d planes</item>
	    <!-- 
	    	%d数字
	    	quantity zero	 例如阿拉伯语中对0的特殊处理
	    			one		 例如俄语和英语中对1的特殊处理
	    			two		 例如威尔士语中对2的特殊处理
	    			few		 例如波兰语中对小数目的特殊处理
	    			many	 例如马耳他语对大数目的特殊处理
	    			other	 当语言要求对一个数字没有特殊待遇时使用
	    	对应代码 getResources().getQuantityString(R.plurals.planes, int num, int num);
	     -->
	</plurals>
	
